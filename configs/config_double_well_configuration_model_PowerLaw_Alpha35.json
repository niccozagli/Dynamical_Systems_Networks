{
  "system": {
    "name": "double_well_network",
    "params": {
      "theta": 1.0,
      "alpha_rot": 0.0
    }
  },
  "network": {
    "name": "configuration_model",
    "params": {
      "n": 1000,
      "degree_distribution": {
        "name": "scale_free_cutoff",
        "params": {
          "alpha": 3.5,
          "k_min": 1,
          "k_max": 100
        }
      }
    }
  },
  "noise": {
    "name": "additive_gaussian",
    "params": {
      "sigma": 0.4
    }
  },
  "integrator": {
    "tmin": 0.0,
    "tmax": 5000.0,
    "dt": 0.01,
    "stats_every": 100,
    "state_every": 1000,
    "write_stats_at_start": true,
    "write_state_at_start": true
  },
  "initial_condition": {
    "type": "normal",
    "std": 1.0
  }
}